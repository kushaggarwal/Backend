    <table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Name</th>
            <th scope="col">Date</th>
            <th scope="col">Thickness</th>
            <th scope="col">Material</th>
            <th scope="col">Time</th>
            <th scope="col">Job</th>
    </tr>
  </thead>
   {{#each persons as |person|}}
  <tbody>
    <tr>
      <td>{{person.name}}</td>
            <td>{{person.ate}}</td>
            <td>{{person.thick}}</td>
